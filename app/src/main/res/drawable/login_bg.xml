<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:state_pressed="true"
            android:shape="rectangle">
            <solid android:color="@color/colorPress"/>
            <corners android:radius="24dp" />
            <stroke android:width="5px" android:color="@color/colorPress" />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <solid android:color="@color/shamrock_green"/>
            <corners android:radius="24dp" />
            <stroke android:width="5px" android:color="@color/shamrock_green" />
        </shape>
    </item>
</selector>
